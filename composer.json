{
    "name": "argentcrusade/stronghold",
    "description": "One Time Tokens package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tzurbaev",
            "email": "zurbaev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "propaganistas/laravel-phone": "^4.2",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^4.0",
        "doctrine/dbal": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "ArgentCrusade\\Stronghold\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArgentCrusade\\Stronghold\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": ["ArgentCrusade\\Stronghold\\Providers\\StrongholdServiceProvider"]
        }
    }
}
